tElementById(\"paused\").classList.toggle(\"hidden\");\n\t}\n});\n\nwindow.addEventListener(\"keyup\", e => {\n\tif ((e.key == \"r\" || e.key == \"R\") && config.mode == \"play\") {\n\t\tif (level.new) {\n\t\t\thopperFunctions.resetHoppers();\n\t\t\tfunctions.setHomeAddresses();\n\t\t\tfunctions.activatePlayMode();\n\t\t} else init();\n\t}\n});\n\nwindow.addEventListener(\"keyup\", e => {\n\tif (\n\t\t(e.key == \"s\" || e.key == \"S\") &&\n\t\tconfig.mode == \"editor\" &&\n\t\tnotTyping()\n\t) {\n\t\tfunctions.setHomeAddresses();\n\t\thopperFunctions.spawnSingleHopper();\n\t\tlevel.new = true;\n\t}\n});\n\nwindow.addEventListener(\"keyup\", e => {\n\tif (\n\t\t(e.key == \"k\" || e.key == \"K\") &&\n\t\tconfig.mode == \"editor\" &&\n\t\tnotTyping()\n\t) {\n\t\thopperFunctions.killAHopper();\n\t\tlevel.new = true;\n\t}\n});\n\nwindow.addEventListener(\"keydown\", e => {\n\tif (e.key == \"Shift\") {\n\t\ttileIcons.forEach(icon => icon.classList.remove(\"selected\"));\n\t\teditor.mode = \"drag\";\n\t}\n});\n\nwindow.addEventListener(\"keyup\", e => {\n\tif (e.key == \"Shift\") {\n\t\teditor.mode = \"none\";\n\t}\n});\n"},"lineCount":null}},"error":null,"hash":"454a35a2feaf53f7a578532a318e4c4e","cacheData":{"env":{}}}